<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- open graph -->
    <meta property="og:title" content="jsist.org - JavaScript Development Conference - Sep 27, 2014 - Istanbul / Turkey">
    <meta property="og:url" content="http://jsist.org">
    <meta property="og:site_name" content="JSist 2014">
    <meta property="og:type" content="website">
    <meta property="og:image" content="http://jsist.org/public/images/jsist-logo.png">
    <meta property="og:description" content="JSist is a JavaScript Development Conference. Sep 27, 2014, Istanbul / Turkey, Koc University. Single track, single day.">

    <meta name="date" content="2014-05-15T08:37:01Z"/>
    <meta name="description" content="JSist is a JavaScript Development Conference. Sep 27, 2014, Istanbul / Turkey, Koc University. Single track, single day."/>
    <meta name="author" content="webBox.io"/>
    <meta name="copyright" content="Copyright (c) 2014 - webBox.io - http://webbox.io"/>
    <meta name="robots" content="all"/>
    <%= partial "partials/icons" %>
    <% data.global.meta and data.global.meta.each do |name, content| %>
      <%= tag :meta, :name => name, :content => content %>
    <% end %>

    <title><%= current_page.data.title || data.global.title || "webBox" %></title>

    <% current_page.data.styles.each do |style| %>
    <%= stylesheet_link_tag style %>
    <% end %>
    <%= stylesheet_link_tag "main" %>
  </head>

  <body class="<%= page_classes %>" data-spy="scroll" data-target=".navbar" data-offset="50">
    <%= yield %>

    <% current_page.data.scripts.each do |script| %>
    <%= javascript_include_tag script %>
    <% end %>
    <%= javascript_include_tag  "main" %>
    <script type="text/javascript">
      if (typeof gaJsHost == 'undefined') {
        var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
        document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
      }
      </script>
      <script type="text/javascript">
        try {
          var pageTracker = _gat._getTracker("<%= data.global.ga %>");
          pageTracker._trackPageview();
        } catch(err) {}
      </script>
  </body>
</html>
