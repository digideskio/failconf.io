<div class="row">
  <% data.team.each do |member| %>
  <div class="col-xs-6 col-md-2 team-member">
    <a href="http://twitter.com/<%= member.twitter %>" class="team-member">
      <img src="<%= member.image %>" alt="@<%= member.twitter %>">
      <div>@<%= member.twitter %></div>
    </a>
  </div>
  <% end %>
</div>
